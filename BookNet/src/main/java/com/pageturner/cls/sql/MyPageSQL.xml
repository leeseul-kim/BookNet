<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
			PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
					"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
					
<mapper namespace="mpSQL">
	<sql id="selMno">
		SELECT
			mno
		FROM
			membertab
		WHERE
			id = #{id}
	</sql>
	
	<select id="selID" parameterType="">
		SELECT
			id
		FROM
			membertab
		WHERE
			mno = #{mno}
	</select>
	
</mapper>